          {
            'include': '#object-types'
          }
          {
            'begin': '\\('
            'beginCaptures':
              '0':
                'name': 'meta.brace.round.java'
            'end': '\\)'
            'endCaptures':
              '0':
                'name': 'meta.brace.round.java'
            'patterns': [
              {
                'include': '#code'
              }
            ]
          }
        ]
      }
      {
        'begin': '{'
        'beginCaptures':
