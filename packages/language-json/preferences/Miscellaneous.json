{
  "name": "Miscellaneous",
  "scope": "source.json",
  "settings": {
    "decreaseIndentPattern": "^\\s*[}\\]],?\\s*$",
    "increaseIndentPattern": "^.*(\\{[^}]*|\\[[^\\]]*)$",
    "smartTypingPairs": [
      [
        "\"",
        "\""
      ],
      [
        "(",
        ")"
      ],
      [
        "{",
        "}"
      ],
      [
        "[",
        "]"
      ]
    ]
  }
}
