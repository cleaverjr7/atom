{
  "name": "Miscellaneous",
  "scope": "source",
  "settings": {
    "decreaseIndentPattern": "^(.*\\*/)?\\s*\\}[;\\s]*$",
    "increaseIndentPattern": "^.*(\\{\\}?[^}\"']*|\\([^)\"']*)$",
    "smartTypingPairs": [
      [
        "\"",
        "\""
      ],
      [
        "(",
        ")"
      ],
      [
        "{",
        "}"
      ],
      [
        "[",
        "]"
      ],
      [
        "“",
        "”"
      ],
      [
        "‘",
        "’"
      ],
      [
        "'",
        "'"
      ],
      [
        "`",
        "`"
      ]
    ],
    "unIndentedLinePattern": "^\\s*$"
  }
}
