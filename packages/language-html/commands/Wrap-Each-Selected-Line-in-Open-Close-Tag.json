{
  "beforeRunningCommand": "nop",
  "command": "perl -pe 's/[\\$`\\\\]/\\\\$&/g; s/([ \\t]*)(.+)/$1<\\${1:li}>$2<\\/\\${1\\/\\\\s.*\\/\\/}>/'",
  "fallbackInput": "line",
  "input": "selection",
  "keyEquivalent": "^@W",
  "name": "Wrap Each Selected Line in Open/Close Tag",
  "output": "insertAsSnippet",
  "scope": "text.html"
}
