'scopeName': 'text.html.mustache'
'name': 'HTML (Mustache)'
'fileTypes': [
  'mustache'
  'handlebars'
  'hbs'
  'stache'
]
'patterns': [
  {
    'include': '#comment'
  }
  {
    'include': '#template'
  }
  {
    'include': 'text.html.basic'
  }
]
'repository':
  'comment':
    'begin': '\\{\\{!'
    'end': '\\}\\}'
    'name': 'comment.block.mustache'
  'template':
    'begin': '\\{\\{[{<>#^/]?'
    'captures':
      '0':
        'name': 'entity.name.tag.mustache'
    'end': '\\}?\\}\\}'
    'name': 'meta.tag.template.mustache'
