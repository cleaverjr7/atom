'.source.js':
  'editor':
    'commentStart': '// '
    'foldEndPattern': '^\\s*\\}|^\\s*\\]|^\\s*\\)'
    'increaseIndentPattern': '(?x)
        \\{ [^}"\']* $
      | \\( [^)"\']* $
      | case[\\s\\S]*: $
      | default: $
      | ^ \\s* if \\s* \\( (?!.*?(\\belse\\b|[;}]\\s*$))
      | ^ \\s* \\}? \\s* else \\s* (?!.*?;\\s*$)
      | ^ \\s* (for|while) \\s* \\( (?!.*?;\\s*$)
      | ^ \\s* do \\s* $
      | ^ \\s* \\{
      '
    'decreaseIndentPattern': '(?x)
        ^ \\s* (\\s* /[*] .* [*]/ \\s*)* [})]
      '
