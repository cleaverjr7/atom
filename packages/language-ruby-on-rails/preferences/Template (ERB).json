{
  "name": "Template (ERB)",
  "scope": "text.html.ruby",
  "settings": {
    "shellVariables": [
      {
        "name": "TM_RAILS_TEMPLATE_START_RUBY_EXPR",
        "value": "<%= "
      },
      {
        "name": "TM_RAILS_TEMPLATE_END_RUBY_EXPR",
        "value": " %>"
      },
      {
        "name": "TM_RAILS_TEMPLATE_START_RUBY_INLINE",
        "value": "<% "
      },
      {
        "name": "TM_RAILS_TEMPLATE_END_RUBY_INLINE",
        "value": " -%>"
      },
      {
        "name": "TM_RAILS_TEMPLATE_END_RUBY_BLOCK",
        "value": "<% end -%>"
      }
    ]
  }
}
